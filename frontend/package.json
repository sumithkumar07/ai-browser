    "@testing-library/user-event": "^14.5.2",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "ahooks": "^3.7.8",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^9.2.0",
    "d3": "^7.8.5",
    "date-fns": "^3.0.6",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^6.1.7",
    "electron-window-state": "^5.0.3",
    "framer-motion": "^10.18.0",
    "immer": "^10.0.3",
    "jinja2": "^0.0.1",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.294.0",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-hook-form": "^7.48.2",
    "react-hotkeys-hook": "^4.4.1",
    "react-redux": "^9.1.0",
    "react-resizable-panels": "^0.0.55",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-speech-recognition": "^3.10.0",
    "react-split": "^2.0.14",
    "react-spring": "^9.7.3",
    "react-use": "^17.4.2",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "reactflow": "^11.11.4",
    "recharts": "^2.10.3",
    "regenerator-runtime": "^0.14.1",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.2.0",
    "three": "^0.159.0",
    "uuid": "^9.0.1",
    "wait-on": "^8.0.4",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    
    "electron": "electron .",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder",
    "build-electron": "yarn build && electron-builder",
    "dist": "yarn build && electron-builder --publish=never",
    "dist-all": "yarn build && electron-builder -mwl",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  },
  "proxy": "http://localhost:8001",
  "main": "electron/main.js",
  "homepage": "./",
  "build": {
    "appId": "com.emergent.aether",
    "productName": "AETHER - Native Chromium Browser",
    "copyright": "Copyright Â© 2025 Emergent AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "!node_modules/.cache/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64", "arm64"]
        }
      ],
      "category": "Network",
      "icon": "build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
